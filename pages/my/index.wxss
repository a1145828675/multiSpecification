.container{
  min-height: 100%;
  overflow: hidden;
  overflow-y: hidden;
}
.userinfo {
	display: flex;
	flex-direction: column;
	padding: 50rpx 0;
	align-items: center;
	background: #e85654;
  width: 750rpx;
}

.userinfo-avatar {
	width: 160rpx;
	height: 160rpx;
	margin: 20rpx;
	border-radius: 50%;
}

.userinfo-nickname {
  margin-top: 20rpx;
  color: #FFF;
}


.version {
  width:100%;
  height: 40rpx;
	line-height: 40rpx;
	font-size: 24rpx;
	text-align: center;  
  padding: 40rpx;
}

.my-item{
    width: 100%;
    background: url(https://cdn.it120.cc/images/weappshop/arrow-right.png) no-repeat 680rpx center;
    background-size: 16rpx auto,750rpx auto;
    padding: 20rpx 0; 
    border-bottom: 1px solid #eee;
    height: 100rpx;
    line-height: 100rpx;
		text-indent: 70rpx;
		position: relative;
}

.my-item button{
	line-height:80rpx;
	text-align: center;
	height:80rpx;
	text-indent: 0;
	
}
.my-item .myBtn-sign{
	margin-left: 200rpx;
	margin-top: 10rpx;
	position: absolute;
	right: 30rpx;
}
.my-phone{
	border:none;
	text-align: center;
	width: 600rpx;
}

.sc{
	text-align: center;
  background: #fff;
  width: 100%;
  font-size: 12px;
  line-height: 16px;
  color: #666;
  padding: 4% 0;
}
.sc image{
  width: 24px;
  height: 24px;
  display: inline-block;
}
.bte{
  border-top: 1px solid #eee;
}
.qbdd{
  background: #fff;
  width: 92%;
  font-size: 12px;
  color: #666;
  padding: 4%;
  
}
.mt110{
  /*border-top: 1px solid #eee;*/
   border-bottom: 1px solid #eee
}

.df{
  display: -webkit-box;
  display: -webkit-flex;
  display: -ms-flexbox;
  display: flex;
}
.df_1{
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  -ms-flex: 1;
  flex: 1;
  -webkit-tap-highlight-color: transparent;
}
.font_12{  font-size: 12px;}

.pay_button{
	padding: 0 30rpx;
	width: 300rpx;
}